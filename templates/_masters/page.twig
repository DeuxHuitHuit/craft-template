{% extends "_masters/html.twig" %}

{% set pageTitle = 'Page title' %}
{% set pageDescription = 'Page description' %}
{% set pageImage = null %}
{% set pageUrl = craft.app.request.absoluteUrl|default %}

{% block site %}
	{% block siteHeader %}
		{% include "_site/header" %}
	{% endblock %}
	<div id="pages">
		<div class="page" data-page-url="{{ pageUrl }}">
			{% block page %}{% endblock %}
		</div>
	</div>
	{% block siteFooter %}
		{% include "_site/footer" %}
	{% endblock %}
{% endblock %}
